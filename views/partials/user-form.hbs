{{#each rows}}
<div class="col-6">
  <div class="form-floating mb-3">
    <input type="text" class="form-control" id="floatingInput" value="{{this.first_name}}" placeholder="first_name"
      name="first_name" required>
    <label for="floatingInput">Name</label>
  </div>
</div>
<div class="col-6">
  <div class="form-floating mb-3">
    <input type="text" class="form-control" id="floatingInput" value="{{this.last_name}}" placeholder="last_name"
      name="last_name" required>
    <label for="floatingInput">Last Name</label>
  </div>
</div>
