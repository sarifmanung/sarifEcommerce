<div class="col-12">
    <div class="form-floating mb-3">
        <input type="text" class="form-control" id="floatingInput" value="{{this.app_id_number}}"
            placeholder="app_id_number" name="app_id_number" required>
        <label for="floatingInput">App ID Number</label>
    </div>
</div>
<div class="col-12">
  <div class="form-floating mb-3">
    <input type="text" class="form-control" id="floatingInput" value="{{this.IC}}" placeholder="IC" name="IC" required>
    <label for="floatingInput">NRIC</label>
  </div>
</div>
<div class="col-12">
  <div class="form-floating mb-3">
    <input type="text" class="form-control" id="floatingInput" value="{{this.phone_number}}" placeholder="Phone"
      name="phone_number">
    <label for="floatingInput">Phone</label>
  </div>
</div>
<div class="col-12">
  <div class="form-floating mb-3">
    <input type="email" class="form-control" id="floatingInput" value="{{this.email}}" placeholder="email" name="email">
    <label for="floatingInput">Email</label>
  </div>
</div>
{{/each}}
<div class="col-12">
  <div class="form-floating mb-3">
    <input type="date" class="form-control" id="edit-user-dob" value="{{format_DOB}}" placeholder="birthday" name="DOB"
      required>
    <label for="floatingInput">Birth</label>
  </div>
</div>
<div class="col-6">
  <div class="form-floating mb-3">
    <input type="date" class="form-control" id="floatingInput" value="{{format_treatment_start_date}}"
      placeholder="treatment_start_date" name="treatment_start_date" required>
    <label for="floatingInput">Start Treatment </label>
  </div>
</div>
<div class="col-6">
  <div class="form-floating mb-3">
    <input type="date" class="form-control" id="floatingInput" value="{{format_treatment_end_date}}"
      placeholder="treatment_end_date" name="treatment_end_date" required>
    <label for="floatingInput">End Treatment</label>
  </div>
</div>
<div class="col-12">
  <div class="form-floating mb-3">
    <input type="date" class="form-control" id="floatingInput" value="{{format_membership_expire_date}}"
      placeholder="membership_expire_date" name="membership_expire_date" required>
    <label for="floatingInput">Expire Date </label>
  </div>
</div>

<div class="col-12 d-grid">
  <button class="btn btn-primary" type="submit">Submit</button>
</div>